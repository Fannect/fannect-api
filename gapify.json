{
   "output": "../fannect-phonegap",
   "assets": [
      {
         "from":"config.xml",
         "to":"{out}/config.xml"
      },
      {
         "from":"assets/js/tree.coffee",
         "to":"{out}/js/tree.js"
      },
      {
         "from":"assets/css/skin.styl",
         "to":"{out}/css/skin.css"
      },
      {
         "from":"assets/css/lib/fannect.css",
         "to":"{out}/css/lib/fannect.css"
      },
      {
         "from":"assets/css/lib/jquerymobile.css",
         "to":"{out}/css/lib/jquerymobile.css"
      },
      {
         "from":"assets/css/lib/normalize.css",
         "to":"{out}/css/lib/normalize.css"
      },
      {
         "from":"public/images",
         "to":"{out}/images"
      },
      {
         "from":"public/fonts",
         "to":"{out}/fonts"
      },
      {
         "from":"public/res",
         "to":"{out}/res"
      }
   ],
   "views": {
      "directory":"views",
      "ignore":["layout.jade"]
   }
}